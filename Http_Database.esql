

CREATE DATABASE MODULE Http_Database
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		
		DECLARE MyRef REFERENCE TO Root.JSON.Data;
		DECLARE DBSchema CHARACTER 'HARSHADA';				
		DECLARE DBTable  CHARACTER 'EMP';
		INSERT INTO Database.{DBSchema}.{DBTable} (ID, NAME, AGE)
					VALUES( MyRef.ID, MyRef.NAME, MyRef.AGE );
		RETURN TRUE;
	END;


END MODULE;
